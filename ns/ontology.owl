    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#decimal"/>
    


    <!-- http://www.w3.org/2001/XMLSchema#int -->


    <owl:Class rdf:about="http://www.w3.org/2001/XMLSchema#int"/>
    


    <!-- https://greeninfratwins.com/ns/grit#Activity -->


    <owl:Class rdf:about="https://greeninfratwins.com/ns/grit#Activity">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#afterActivity"/>
                <owl:someValuesFrom rdf:resource="https://greeninfratwins.com/ns/grit#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasIndicatorSet"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://greeninfratwins.com/ns/grit#IndicatorSet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The activity that is performed in a particularly sequence within a measure or measure variant.</rdfs:comment>
        <rdfs:label>Activity</rdfs:label>
    </owl:Class>
    


    <!-- https://greeninfratwins.com/ns/grit#Assessment -->


    <owl:Class rdf:about="https://greeninfratwins.com/ns/grit#Assessment">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasMeasure"/>
                <owl:someValuesFrom rdf:resource="https://greeninfratwins.com/ns/grit#Measure"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#isDefinedBy"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://greeninfratwins.com/ns/grit#SpecifiedUseCase"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>The use case that is performed in a DT.</rdfs:comment>
        <rdfs:label>Assessment defined by a specified Use Case</rdfs:label>
    </owl:Class>
    


    <!-- https://greeninfratwins.com/ns/grit#ComplexComputedValueIndicator -->


    <owl:Class rdf:about="https://greeninfratwins.com/ns/grit#ComplexComputedValueIndicator">
        <rdfs:subClassOf rdf:resource="https://greeninfratwins.com/ns/grit#ValueIndicator"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasVariable"/>
                <owl:someValuesFrom rdf:resource="https://greeninfratwins.com/ns/grit#IndicatorVariable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasExternalCalculationApplication"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="https://greeninfratwins.com/ns/grit#ConstantValueIndicator"/>
        <owl:disjointWith rdf:resource="https://greeninfratwins.com/ns/grit#SimpleComputedValueIndicator"/>
        <rdfs:comment>An indicator that cannot be calculated by just a given formula but in expert software.</rdfs:comment>
        <rdfs:label>Complex computed value indicator</rdfs:label>
    </owl:Class>
    


    <!-- https://greeninfratwins.com/ns/grit#ConstantValueIndicator -->


