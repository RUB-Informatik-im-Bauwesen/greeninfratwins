    <!-- https://greeninfratwins.com/ns/grit#Activity -->


    <owl:Class rdf:about="https://greeninfratwins.com/ns/grit#Activity">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#afterActivity"/>
                <owl:someValuesFrom rdf:resource="https://greeninfratwins.com/ns/grit#Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasIfcGuid"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasIndicatorSet"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://greeninfratwins.com/ns/grit#IndicatorSet"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://greeninfratwins.com/ns/grit#hasIcddModelID"/>
                <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
