        <terms:rights>Copyright 2025 by Chair of Computing in Engineering, Ruhr University Bochum</terms:rights>
        <terms:source>https://www.greeninfratwins.com</terms:source>
        <terms:title xml:lang="en">GreenInfraTwins (GRIT) Ontology</terms:title>
        <vann:preferredNamespacePrefix>grit</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:resource="https://greeninfratwins.com/ns/grit#"/>
        <owl:versionInfo>0.3</owl:versionInfo>
    </owl:Ontology>
